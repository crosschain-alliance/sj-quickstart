{
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4
      }
    }
  ],
  "plugins": ["prettier-plugin-solidity"],
  "printWidth": 120,
  "proseWrap": "always",
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all"
}
